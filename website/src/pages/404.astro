---
import Layout from "../components/Layout.astro";
import Link from "../components/Link.astro";
---

<Layout title="Page Not Found">
  <div class="space-y-2 font-serif text-2xl text-pretty">
    <h2 class="font-bold">Page Not Found</h2>
    <p>The webpage you were looking for doesn't seem to exist.</p>
    <p>
      Try returning to the <Link href="/">home page</Link>.
    </p>
  </div>
</Layout>
