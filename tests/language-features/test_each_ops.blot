// Test all each operations
a = each([2, 4, 6])
b = each([1, 2, 3])

print("a: {}", collect(a))
print("b: {}", collect(b))

print("a + b: {}", collect(a + b))
print("a - b: {}", collect(a - b))
print("a * b: {}", collect(a * b))
print("a / b: {}", collect(a / b))

// Test with scalars
print("a + 1: {}", collect(a + 1))
print("a * 2: {}", collect(a * 2))